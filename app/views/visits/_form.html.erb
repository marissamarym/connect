<%= simple_form_for(@visit) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :start_time %>
    <%= f.input :end_time %>
    <%= f.input :type %>
    <%= f.input :notes %>
    <%= f.association :prospie %>
    <%= f.association :school %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
